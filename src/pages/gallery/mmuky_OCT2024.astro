---
import type { ImageMetadata } from "astro";
import GalleryLayout from "../../layouts/GalleryLayout.astro";

var images = import.meta.glob<{ default: ImageMetadata }>("../../../public/images/albums/_mmuky_OCT2024/*.webp");
const fontColor = "#ffb347";
const backgroundColor = "black";
const albumName = "Mook October 2024";
---

<GalleryLayout title={albumName} backgroundColor={backgroundColor} fontColor={fontColor} imageData={images} />
